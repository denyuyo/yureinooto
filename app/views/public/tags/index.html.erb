<div class="container" style="font-family: 'Yusei Magic', sans-serif; margin-top: 33px; margin-bottom: 22px;">
  <div class="row">
    <% @tags.each do |tag|%>
      <%= link_to tag_path(tag), class: "btn btn-outline-info rounded-pill m-1" do %>
        <i class="fas fa-tag"></i>
        <%= tag.tag_name %>
      <% end %>
    <% end %>
  </div>
</div>